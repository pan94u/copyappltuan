		dataType:"json",
		type:"get",
		url:"/getJobs",
		success:function (data) {
      document.title = "胖胖数码"+data.result["date"];
      $("#loadingtips").fadeOut(0,function(){
        $("#loadingtips").removeClass("alert-warning");
        $("#loadingtips").addClass("alert-success");
        $("#loadingtips").text("加载完成!");
        $("#loadingtips").fadeIn(1000,function(){
          $("#loadingtips").fadeOut(1000);
        });
      });
      $(".box")[0].innerHTML = "";
      $(".graybox").remove();
      $(".box").removeClass("graybox1");
      $("#largeloading").remove();
			$(".box").append(data.result["html"]);
		},
    error:function(err){
      $("#tips").removeClass("alert-warning");
      $("#tips").addClass("alert-danger");
      $("#tips").text("获取失败!");
    }
	})

	function GetRequest() {
		var url = location.search; //获取url中"?"符后的字串
		var theRequest = new Object();
		if (url.indexOf("?") != -1) {
		  var str = url.substr(1);
		  strs = str.split("&");
		  for(var i = 0; i < strs.length; i ++) {
		     theRequest[strs[i].split("=")[0]]=(strs[i].split("=")[1]);
		  }
		}
   		return theRequest;
	}
</script>

